#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/bin/node_modules:/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/node_modules:/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/esbuild@0.15.18/node_modules:/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/bin/node_modules:/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/node_modules:/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/esbuild@0.15.18/node_modules:/Volumes/extdisk/project/web3/polkadot-hackathon-2024/singapore/10-FlyAIMusic/src/web/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
